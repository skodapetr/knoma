    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
